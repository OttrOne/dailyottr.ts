{
  "name": "dailyottr",
  "version": "1.7.0",
  "description": "DailyOttr bot based on LexBot TypeScript!",
  "scripts": {
    "prebuild": "node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "build": "tsc",
    "lint": "eslint \"**/*.ts\"",
    "start": "node dist/index.js",
    "debug": "nodemon src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OttrOne/dailyottr.ts.git"
  },
  "keywords": [
    "Discord",
    "Bot",
    "TypeScript"
  ],
  "author": "Alex Ottr <alex@ottr.one>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OttrOne/dailyottr.ts/issues"
  },
  "homepage": "https://github.com/OttrOne/dailyottr.ts#readme",
  "dependencies": {
    "discord-api-types": "^0.35.0",
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "mongoose": "^6.0.4",
    "node-fetch": "^3.0.0",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.24.2",
    "nodemon": "^2.0.18"
  }
}
